<!DOCTYPE html>
<% if signed_in? %>
  <table class="front" summary="Pour visiteur identifie">
    <tr>
      <td class="main">
        <h1 class="micropost">Nouveau message</h1>
        <%= render 'shared/micropost_form' %>
        <%= render 'shared/feed' %>
   
      </td>
      <td class="sidebar round">
        <%= render 'shared/user_info' %>
        <%= render 'shared/stats' %>
      </td>
    </tr>
  </table>
    

<% end %>