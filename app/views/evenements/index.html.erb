<%# if signed_in?  %>
	<table class="evenement" summary="Pour visiteur identifie">
		<tr>
			<td class="main">
				<% $index_pro = 0 %>
				<ul class="users">
					<br/>
					<% if @evenements.present?  %><h2>Evenements</h2>
						<% $sav_date = "" %>
						<%= render @evenements %>
						<br><%= will_paginate %></br>
