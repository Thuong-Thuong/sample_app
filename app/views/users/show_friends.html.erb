<table summary="Information a propos des amis et des invitations
	<tr>
		<td class="main">
			<h1><%= @titre %></h1>
			<% unless @users.empty? %>
				<ul class="users">
					<%= render @users %>
				</ul>
				<%= will_paginate @users %>
			<% end %>
		</td>
		<td class="sidebar round">
			<%= render 'shared/user_info' %>
			<%= render 'shared/statsfriends' %>
		</td>

	</tr>
</table>