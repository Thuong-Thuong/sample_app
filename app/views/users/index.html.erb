<% if signed_in? %>
  <table class="front" summary="Pour visiteur identifie">
    <tr>
      <td class="main">
		<h1>Tous les utilisateurs</h1>
		<%= will_paginate %>
		<ul 	class="users">
 			<%= render @users %>
		</ul>
		<%= will_paginate %>
      </td>

      <td class="sidebar round">
        <%= render 'shared/user_info' %>
        <%= render 'shared/stats' %>
      </td>
    </tr>
  </table>
    
<% end %>
