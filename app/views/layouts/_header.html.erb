<header>
	<ul>  
		<%= link_to logo, root_path %>
	</ul>
	<nav class="round">
		<ul>
			<li><%= link_to "Accueil", root_path %></li>
      		<% if signed_in? %>
				<li><%= link_to "Utilisateurs", users_path %></li>
				<li><%= link_to "Profil", current_user %></li>
				<li><%= link_to "Reglages", edit_user_path(current_user) %></li>
				<li><%= link_to "Microposts", textperso_path %></li>
				<% if current_user.admin? %>
					<li><%= link_to "SignalPro", signalindex_path %></li>
					<li><%= link_to "SignalEven", signalevenindex_path %></li>
				<% end %>
				<li><%= link_to "Evenements", evenements_path %></li>
			<% end %>
			<li><%= link_to "Aide", help_path %></li>
			<% if signed_in? %>
				<li><%= link_to "Deconnexion", signout_path, :method => :delete %>  </li>  
			<% else %>
				<li><%= link_to "S'identifier", signin_path %></li>
			<% end %>
		</ul>
	</nav>
</header>