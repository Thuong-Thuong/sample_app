<tr>
	<td class="gravatar">
		<%= gravatar_pour(feed_item_evenmt.user, :size => 30) %>
	</td>
	<td class="evenement">
		<span class="content"></strong><%= link_to feed_item_evenmt.titre, inscription_path %></span> <br />
		<!--<span class="timestamp">
			Post√© il y a <%= time_ago_in_words(feed_item_evenmt.created_at) %>.
		</span> --> 
	</td>
	<% if current_user?(feed_item_evenmt.user) %>
		<td>    
			<%= button_to "Supprimer", feed_item_evenmt, {:onclick => "return confirm('Are you sure to delete evenement #{feed_item_evenmt.titre}?')", :method => :delete, :class => 'round', :title => feed_item_evenmt.titre}%>
		</td>
	<% else %>
		<td>    
			<%# = button_to "Inscription", feed_item_evenmt, :method => :build, :class => 'round', :title => feed_item_evenmt.titre %>
		</td>  
	<% end %>
</tr>