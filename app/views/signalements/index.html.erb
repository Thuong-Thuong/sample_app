			<td class="main">
				<% $index_pro = 1 %>
					<br/><br/><h2>Tous les signalements</h2>
				<ul class="users">
					<%= render @signalements %>
				</ul>
				<%= will_paginate %>
			</td>
			<td class="sidebar round">
				<br/><br/><br/>	<%= render 'shared/user_info' %>
